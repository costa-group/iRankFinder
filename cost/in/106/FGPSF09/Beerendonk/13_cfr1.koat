(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B A_P B_P)
(RULES 
  pyRinit(A,B) -> Com_1(n_start(A,B))
  n_eval___1(A,B) -> Com_1(n_eval___1(A -1, B)) :|: -A + B >= 0 && A -1 >= 0 && -A + B >= 0
  n_eval___2(A,B) -> Com_1(n_eval___1(A -1, B)) :|: A -1 >= 0 && -A + B >= 0
  n_eval___2(A,B) -> Com_1(n_eval___1(B, B)) :|: A -1 >= 0 && A -B -1 >= 0
  n_start(A,B) -> Com_1(n_eval___2(A, B))
)
