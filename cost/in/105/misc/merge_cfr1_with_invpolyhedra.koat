(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B A_P B_P)
(RULES 
  pyRinit(A,B) -> Com_1(n_start(A,B))
  n_merge___1(A,B) -> Com_1(n_merge___1(A, B -1)) :|: A -1 >= 0 && -1 + A >= 0 && B >= 0 && A -1 >= 0 && B -1 >= 0 && B >= 0 && -1 + A >= 0
  n_merge___1(A,B) -> Com_1(n_merge___2(A -1, B)) :|: A -1 >= 0 && -1 + A >= 0 && B >= 0 && A -1 >= 0 && B -1 >= 0 && B >= 0 && -1 + A >= 0
  n_merge___2(A,B) -> Com_1(n_merge___1(A, B -1)) :|: B -1 >= 0 && A >= 0 && -1 + B >= 0 && A -1 >= 0 && B -1 >= 0 && -1 + B >= 0 && A >= 0
  n_merge___2(A,B) -> Com_1(n_merge___2(A -1, B)) :|: B -1 >= 0 && A >= 0 && -1 + B >= 0 && A -1 >= 0 && B -1 >= 0 && -1 + B >= 0 && A >= 0
  n_merge___3(A,B) -> Com_1(n_merge___1(A, B -1)) :|: A -1 >= 0 && B -1 >= 0
  n_merge___3(A,B) -> Com_1(n_merge___2(A -1, B)) :|: A -1 >= 0 && B -1 >= 0
  n_start(A,B) -> Com_1(n_merge___3(A, B))
)
