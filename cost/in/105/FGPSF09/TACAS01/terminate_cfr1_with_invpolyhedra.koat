(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A_P B_P C_P)
(RULES 
  pyRinit(A,B,C) -> Com_1(n_start(A,B,C))
  n_eval___1(A,B,C) -> Com_1(n_eval___1(C, B -1, A + 1)) :|: 101 -C >= 0 && 1 -A + B >= 0 && B -C >= 0 && -A + 100 >= 0 && 101 -C >= 0 && 1 -A + B >= 0
  n_eval___2(A,B,C) -> Com_1(n_eval___1(C, B -1, A + 1)) :|: B -C >= 0 && -A + 100 >= 0
  n_start(A,B,C) -> Com_1(n_eval___2(A, B, C))
)
