(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A A_P)
(RULES 
  pyRinit(A) -> Com_1(n_start(A))
  n_eval___1(A) -> Com_1(n_eval___1(A_P)) :|: 2 + A >= 0 && A_P + 2 >= 0 && A -A_P -3 = 0
  n_eval___2(A) -> Com_1(n_eval___1(A_P)) :|: A_P + 2 >= 0 && A -A_P -3 = 0
  n_start(A) -> Com_1(n_eval___2(A))
)
