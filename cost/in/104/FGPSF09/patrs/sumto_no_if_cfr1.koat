(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B A_P B_P NoDet1 NoDet0)
(RULES 
  pyRinit(A,B) -> Com_1(n_start(A,B))
  n_start(A,B) -> Com_1(n_sumto___2(A, B))
  n_sumto___2(A,B) -> Com_1(n_end___1(NoDet0, NoDet1)) :|: A -B -1 >= 0
  n_sumto___2(A,B) -> Com_1(n_sumto___2(A + 1, B)) :|: -A + B >= 0
)
