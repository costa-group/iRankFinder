(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A_P B_P C_P NoDet0)
(RULES 
  pyRinit(A,B,C) -> Com_1(n_start(A,B,C))
  n_eval___1(A,B,C) -> Com_1(n_eval___1(A + 1, A + B, C)) :|: -2 + 2 * A -B >= 0 && -2 + 2 * A -B >= 0 && -2 + 2 * A -B >= 0 && A -B >= 0 && -2 + 2 * A -B >= 0
  n_eval___1(A,B,C) -> Com_1(n_eval___2(A -C, NoDet0, C -1)) :|: -2 + 2 * A -B >= 0 && -2 + 2 * A -B >= 0 && -2 + 2 * A -B >= 0 && A -B >= 0 && -2 + 2 * A -B >= 0
  n_eval___2(A,B,C) -> Com_1(n_eval___1(A + 1, A + B, C)) :|: A -B >= 0
  n_eval___2(A,B,C) -> Com_1(n_eval___2(A -C, NoDet0, C -1)) :|: A -B >= 0
  n_start(A,B,C) -> Com_1(n_eval___2(A, B, C))
)
