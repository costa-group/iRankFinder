(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B A_P B_P NoDet1 NoDet0)
(RULES 
  pyRinit(A,B) -> Com_1(n_l0(A,B))
  n_l0(A,B) -> Com_1(n_l1___3(0, B))
  n_l1___2(A,B) -> Com_1(n_l1___2(A + 1, B -1)) :|: -1 + A >= 0 && B >= 0 && -1 + A >= 0 && B >= 0 && A >= 0 && -1 + A >= 0 && B >= 0 && -1 + A >= 0 && B >= 0 && A >= 0 && A >= 0 && B -1 >= 0 && B >= 0 && -1 + A >= 0
  n_l1___2(A,B) -> Com_1(n_l2___1(NoDet0, NoDet1)) :|: -1 + A >= 0 && B >= 0 && -1 + A >= 0 && B >= 0 && A >= 0 && -1 + A >= 0 && B >= 0 && -1 + A >= 0 && B >= 0 && A >= 0 && A >= 0 && -B >= 0 && B >= 0 && -1 + A >= 0
  n_l1___3(A,B) -> Com_1(n_l1___2(A + 1, B -1)) :|: A = 0 && A = 0 && A = 0 && A = 0 && A >= 0 && A >= 0 && A >= 0 && B -1 >= 0 && A = 0
  n_l1___3(A,B) -> Com_1(n_l2___1(NoDet0, NoDet1)) :|: A = 0 && A = 0 && A = 0 && A = 0 && A >= 0 && A >= 0 && A >= 0 && -B >= 0 && A = 0
)
