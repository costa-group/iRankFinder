(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A_P B_P C_P)
(RULES 
  pyRinit(A,B,C) -> Com_1(n_start(A,B,C))
  n_start(A,B,C) -> Com_1(n_zip3___2(A, B, C))
  n_zip3___1(A,B,C) -> Com_1(n_zip3___1(A -1, B -1, C -1)) :|: A >= 0 && B >= 0 && C >= 0 && A >= 0 && B >= 0 && C >= 0 && A >= 0 && B >= 0 && C >= 0 && A >= 0 && B >= 0 && C >= 0 && A -1 >= 0 && B -1 >= 0 && C -1 >= 0
  n_zip3___2(A,B,C) -> Com_1(n_zip3___1(A -1, B -1, C -1)) :|: A -1 >= 0 && B -1 >= 0 && C -1 >= 0
)
