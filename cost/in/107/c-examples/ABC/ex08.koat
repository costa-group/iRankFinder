(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A' B' C' NoDet2 NoDet1 NoDet0)
(RULES 
  pyRinit(A,B,C) -> Com_1(evalfstart(A,B,C))
  evalfbb1in(A,B,C) -> Com_1(evalfbb2in(A, B, C -1))
  evalfbb2in(A,B,C) -> Com_1(evalfbb1in(A, B, C)) :|: C -1 >= 0
  evalfbb2in(A,B,C) -> Com_1(evalfbb3in(A, B, C)) :|: -C >= 0
  evalfbb3in(A,B,C) -> Com_1(evalfbb4in(A, B -1, C))
  evalfbb4in(A,B,C) -> Com_1(evalfbb2in(A, B, A)) :|: B -1 >= 0
  evalfbb4in(A,B,C) -> Com_1(evalfreturnin(A, B, C)) :|: -B >= 0
  evalfentryin(A,B,C) -> Com_1(evalfbb4in(B, A, C))
  evalfreturnin(A,B,C) -> Com_1(evalfstop(NoDet0, NoDet1, NoDet2))
  evalfstart(A,B,C) -> Com_1(evalfentryin(A, B, C))
)
