(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C D A' B' C' D' NoDet0 NoDet1 E NoDet2 NoDet3)
(RULES 
  pyRinit(A,B,C,D) -> Com_1(evalEx1start(A,B,C,D))
  evalEx1bb1in(A,B,C,D) -> Com_1(evalEx1bb4in(A, B, C + 1, D))
  evalEx1bb1in(A,B,C,D) -> Com_1(evalEx1bb4in(A, B, C, D -1)) :|: -1 -E >= 0
  evalEx1bb1in(A,B,C,D) -> Com_1(evalEx1bb4in(A, B, C, D -1)) :|: E -1 >= 0
  evalEx1bb4in(A,B,C,D) -> Com_1(evalEx1bb1in(A, B, C, D)) :|: D -C -1 >= 0
  evalEx1bb4in(A,B,C,D) -> Com_1(evalEx1bb5in(A, B, C, D)) :|: C -D >= 0
  evalEx1bb5in(A,B,C,D) -> Com_1(evalEx1bb6in(A + 1, D, C, D))
  evalEx1bb6in(A,B,C,D) -> Com_1(evalEx1bbin(A, B, C, D)) :|: B -A -1 >= 0
  evalEx1bb6in(A,B,C,D) -> Com_1(evalEx1returnin(A, B, C, D)) :|: A -B >= 0
  evalEx1bbin(A,B,C,D) -> Com_1(evalEx1bb4in(A, B, A + 1, B))
  evalEx1entryin(A,B,C,D) -> Com_1(evalEx1bb6in(0, A, C, D))
  evalEx1returnin(A,B,C,D) -> Com_1(evalEx1stop(NoDet0, NoDet1, NoDet2, NoDet3))
  evalEx1start(A,B,C,D) -> Com_1(evalEx1entryin(A, B, C, D))
)
