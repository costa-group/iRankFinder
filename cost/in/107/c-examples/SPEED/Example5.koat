(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A' B' C' NoDet0 NoDet2 NoDet1)
(RULES 
  pyRinit(A,B,C) -> Com_1(evalfstart(A,B,C))
  evalfbb1in(A,B,C) -> Com_1(evalfbb3in(A, B, C + 1))
  evalfbb2in(A,B,C) -> Com_1(evalfbb3in(A, B, C -1))
  evalfbb3in(A,B,C) -> Com_1(evalfbbin(A, B, C)) :|: C -1 >= 0 && B -C -1 >= 0
  evalfbb3in(A,B,C) -> Com_1(evalfreturnin(A, B, C)) :|: -C >= 0
  evalfbb3in(A,B,C) -> Com_1(evalfreturnin(A, B, C)) :|: C -B >= 0
  evalfbbin(A,B,C) -> Com_1(evalfbb1in(A, B, C)) :|: A -1 >= 0
  evalfbbin(A,B,C) -> Com_1(evalfbb2in(A, B, C)) :|: -A >= 0
  evalfentryin(A,B,C) -> Com_1(evalfbb3in(C, B, A)) :|: A -1 >= 0 && B -A -1 >= 0
  evalfreturnin(A,B,C) -> Com_1(evalfstop(NoDet0, NoDet1, NoDet2))
  evalfstart(A,B,C) -> Com_1(evalfentryin(A, B, C))
)
