(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A' B' C' NoDet1 D NoDet2 NoDet0)
(RULES 
  pyRinit(A,B,C) -> Com_1(evalfstart(A,B,C))
  evalfbb1in(A,B,C) -> Com_1(evalfbb3in(A, B -1, C + 1))
  evalfbb3in(A,B,C) -> Com_1(evalfbb4in(A, B, C)) :|: B -1 >= 0
  evalfbb3in(A,B,C) -> Com_1(evalfreturnin(A, B, C)) :|: -B >= 0
  evalfbb4in(A,B,C) -> Com_1(evalfbbin(A, B, C)) :|: -1 -D >= 0
  evalfbb4in(A,B,C) -> Com_1(evalfbbin(A, B, C)) :|: D -1 >= 0
  evalfbb4in(A,B,C) -> Com_1(evalfreturnin(A, B, C))
  evalfbbin(A,B,C) -> Com_1(evalfbb1in(A, B, C)) :|: A -C -1 >= 0
  evalfbbin(A,B,C) -> Com_1(evalfbb3in(A, B, 0)) :|: C -A >= 0
  evalfentryin(A,B,C) -> Com_1(evalfbb3in(B, A, 0)) :|: A -1 >= 0 && B -1 >= 0
  evalfreturnin(A,B,C) -> Com_1(evalfstop(NoDet0, NoDet1, NoDet2))
  evalfstart(A,B,C) -> Com_1(evalfentryin(A, B, C))
)
