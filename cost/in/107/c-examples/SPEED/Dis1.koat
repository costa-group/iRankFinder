(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C D A' B' C' D' NoDet0 NoDet1 NoDet2 NoDet3)
(RULES 
  pyRinit(A,B,C,D) -> Com_1(evalDis1start(A,B,C,D))
  evalDis1bb1in(A,B,C,D) -> Com_1(evalDis1bb3in(A, B, C, D + 1))
  evalDis1bb2in(A,B,C,D) -> Com_1(evalDis1bb3in(A, B + 1, C, D))
  evalDis1bb3in(A,B,C,D) -> Com_1(evalDis1bbin(A, B, C, D)) :|: A -B -1 >= 0
  evalDis1bb3in(A,B,C,D) -> Com_1(evalDis1returnin(A, B, C, D)) :|: B -A >= 0
  evalDis1bbin(A,B,C,D) -> Com_1(evalDis1bb1in(A, B, C, D)) :|: C -D -1 >= 0
  evalDis1bbin(A,B,C,D) -> Com_1(evalDis1bb2in(A, B, C, D)) :|: D -C >= 0
  evalDis1entryin(A,B,C,D) -> Com_1(evalDis1bb3in(B, A, D, C))
  evalDis1returnin(A,B,C,D) -> Com_1(evalDis1stop(NoDet0, NoDet1, NoDet2, NoDet3))
  evalDis1start(A,B,C,D) -> Com_1(evalDis1entryin(A, B, C, D))
)
