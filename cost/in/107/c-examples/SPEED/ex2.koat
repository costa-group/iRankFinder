(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C D A' B' C' D' NoDet2 NoDet3 NoDet1 NoDet0)
(RULES 
  pyRinit(A,B,C,D) -> Com_1(evalfstart(A,B,C,D))
  evalfbb1in(A,B,C,D) -> Com_1(evalfbb3in(A + 1, B, C, D))
  evalfbb2in(A,B,C,D) -> Com_1(evalfbb3in(0, B + 1, C, D))
  evalfbb3in(A,B,C,D) -> Com_1(evalfbbin(A, B, C, D)) :|: C -B -1 >= 0
  evalfbb3in(A,B,C,D) -> Com_1(evalfreturnin(A, B, C, D)) :|: B -C >= 0
  evalfbbin(A,B,C,D) -> Com_1(evalfbb1in(A, B, C, D)) :|: D -A -1 >= 0
  evalfbbin(A,B,C,D) -> Com_1(evalfbb2in(A, B, C, D)) :|: A -D >= 0
  evalfentryin(A,B,C,D) -> Com_1(evalfbb3in(0, 0, C, D))
  evalfreturnin(A,B,C,D) -> Com_1(evalfstop(NoDet0, NoDet1, NoDet2, NoDet3))
  evalfstart(A,B,C,D) -> Com_1(evalfentryin(A, B, C, D))
)
