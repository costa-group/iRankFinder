(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C D A' B' C' D' NoDet0 NoDet3 NoDet2 NoDet1)
(RULES 
  pyRinit(A,B,C,D) -> Com_1(evalspeedpldi3start(A,B,C,D))
  evalspeedpldi3bb2in(A,B,C,D) -> Com_1(evalspeedpldi3bb3in(A, B, C, D)) :|: A -C -1 >= 0
  evalspeedpldi3bb2in(A,B,C,D) -> Com_1(evalspeedpldi3bb4in(A, B, C, D)) :|: C -A >= 0
  evalspeedpldi3bb3in(A,B,C,D) -> Com_1(evalspeedpldi3bb5in(A, B, C + 1, D))
  evalspeedpldi3bb4in(A,B,C,D) -> Com_1(evalspeedpldi3bb5in(A, B, 0, D + 1))
  evalspeedpldi3bb5in(A,B,C,D) -> Com_1(evalspeedpldi3bb2in(A, B, C, D)) :|: B -D -1 >= 0
  evalspeedpldi3bb5in(A,B,C,D) -> Com_1(evalspeedpldi3returnin(A, B, C, D)) :|: D -B >= 0
  evalspeedpldi3entryin(A,B,C,D) -> Com_1(evalspeedpldi3bb5in(A, B, 0, 0)) :|: A -1 >= 0 && B -A -1 >= 0
  evalspeedpldi3entryin(A,B,C,D) -> Com_1(evalspeedpldi3returnin(A, B, C, D)) :|: -A >= 0
  evalspeedpldi3entryin(A,B,C,D) -> Com_1(evalspeedpldi3returnin(A, B, C, D)) :|: A -B >= 0
  evalspeedpldi3returnin(A,B,C,D) -> Com_1(evalspeedpldi3stop(NoDet0, NoDet1, NoDet2, NoDet3))
  evalspeedpldi3start(A,B,C,D) -> Com_1(evalspeedpldi3entryin(A, B, C, D))
)
