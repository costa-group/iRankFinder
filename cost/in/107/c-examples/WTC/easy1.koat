(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B A' B' NoDet1 NoDet0)
(RULES 
  pyRinit(A,B) -> Com_1(evaleasy1start(A,B))
  evaleasy1bb1in(A,B) -> Com_1(evaleasy1bb3in(A + 1, B))
  evaleasy1bb2in(A,B) -> Com_1(evaleasy1bb3in(A + 2, B))
  evaleasy1bb3in(A,B) -> Com_1(evaleasy1bbin(A, B)) :|: 39 -A >= 0
  evaleasy1bb3in(A,B) -> Com_1(evaleasy1returnin(A, B)) :|: A -40 >= 0
  evaleasy1bbin(A,B) -> Com_1(evaleasy1bb1in(A, B)) :|: B = 0
  evaleasy1bbin(A,B) -> Com_1(evaleasy1bb2in(A, B)) :|: -1 -B >= 0
  evaleasy1bbin(A,B) -> Com_1(evaleasy1bb2in(A, B)) :|: B -1 >= 0
  evaleasy1entryin(A,B) -> Com_1(evaleasy1bb3in(0, B))
  evaleasy1returnin(A,B) -> Com_1(evaleasy1stop(NoDet0, NoDet1))
  evaleasy1start(A,B) -> Com_1(evaleasy1entryin(A, B))
)
