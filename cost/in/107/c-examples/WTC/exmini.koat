(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A' B' C' NoDet0 NoDet2 NoDet1)
(RULES 
  pyRinit(A,B,C) -> Com_1(evalexministart(A,B,C))
  evalexminibb1in(A,B,C) -> Com_1(evalexminibbin(A, B, C)) :|: 100 -B >= 0 && A -C >= 0
  evalexminibb1in(A,B,C) -> Com_1(evalexminireturnin(A, B, C)) :|: B -101 >= 0
  evalexminibb1in(A,B,C) -> Com_1(evalexminireturnin(A, B, C)) :|: C -A -1 >= 0
  evalexminibbin(A,B,C) -> Com_1(evalexminibb1in(A -1, C, B + 1))
  evalexminientryin(A,B,C) -> Com_1(evalexminibb1in(B, A, C))
  evalexminireturnin(A,B,C) -> Com_1(evalexministop(NoDet0, NoDet1, NoDet2))
  evalexministart(A,B,C) -> Com_1(evalexminientryin(A, B, C))
)
