(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A' B' C' NoDet2 D NoDet0 NoDet1)
(RULES 
  pyRinit(A,B,C) -> Com_1(evalrsdstart(A,B,C))
  evalrsdbb1in(A,B,C) -> Com_1(evalrsdbb2in(A, B, C)) :|: -1 -D >= 0
  evalrsdbb1in(A,B,C) -> Com_1(evalrsdbb2in(A, B, C)) :|: D -1 >= 0
  evalrsdbb1in(A,B,C) -> Com_1(evalrsdbb3in(A, B, C))
  evalrsdbb2in(A,B,C) -> Com_1(evalrsdbb4in(A, B, C -1))
  evalrsdbb3in(A,B,C) -> Com_1(evalrsdbb4in(A, B -1, B -1))
  evalrsdbb4in(A,B,C) -> Com_1(evalrsdbb1in(A, B, C)) :|: C -A >= 0
  evalrsdbb4in(A,B,C) -> Com_1(evalrsdreturnin(A, B, C)) :|: A -C -1 >= 0
  evalrsdbbin(A,B,C) -> Com_1(evalrsdbb4in(A, 2 * A, 2 * A))
  evalrsdentryin(A,B,C) -> Com_1(evalrsdbbin(A, B, C)) :|: A >= 0
  evalrsdentryin(A,B,C) -> Com_1(evalrsdreturnin(A, B, C)) :|: -1 -A >= 0
  evalrsdreturnin(A,B,C) -> Com_1(evalrsdstop(NoDet0, NoDet1, NoDet2))
  evalrsdstart(A,B,C) -> Com_1(evalrsdentryin(A, B, C))
)
