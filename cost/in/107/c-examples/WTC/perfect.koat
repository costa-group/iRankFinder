(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C D A' B' C' D' NoDet1 NoDet3 NoDet2 NoDet0)
(RULES 
  pyRinit(A,B,C,D) -> Com_1(evalperfectstart(A,B,C,D))
  evalperfectbb1in(A,B,C,D) -> Com_1(evalperfectbb8in(A, A, A -1, D))
  evalperfectbb3in(A,B,C,D) -> Com_1(evalperfectbb4in(A, B, C, D -C))
  evalperfectbb4in(A,B,C,D) -> Com_1(evalperfectbb3in(A, B, C, D)) :|: D -C >= 0
  evalperfectbb4in(A,B,C,D) -> Com_1(evalperfectbb5in(A, B, C, D)) :|: C -D -1 >= 0
  evalperfectbb5in(A,B,C,D) -> Com_1(evalperfectbb8in(A, B, C -1, D)) :|: -1 -D >= 0
  evalperfectbb5in(A,B,C,D) -> Com_1(evalperfectbb8in(A, B, C -1, D)) :|: D -1 >= 0
  evalperfectbb5in(A,B,C,D) -> Com_1(evalperfectbb8in(A, B -C, C -1, D)) :|: D = 0
  evalperfectbb8in(A,B,C,D) -> Com_1(evalperfectbb4in(A, B, C, A)) :|: C -1 >= 0
  evalperfectbb8in(A,B,C,D) -> Com_1(evalperfectbb9in(B, B, C, D)) :|: -C >= 0
  evalperfectbb9in(A,B,C,D) -> Com_1(evalperfectreturnin(A, B, C, D)) :|: -1 -A >= 0
  evalperfectbb9in(A,B,C,D) -> Com_1(evalperfectreturnin(A, B, C, D)) :|: A -1 >= 0
  evalperfectbb9in(A,B,C,D) -> Com_1(evalperfectreturnin(A, B, C, D)) :|: A = 0
  evalperfectentryin(A,B,C,D) -> Com_1(evalperfectbb1in(A, B, C, D)) :|: A -2 >= 0
  evalperfectentryin(A,B,C,D) -> Com_1(evalperfectreturnin(A, B, C, D)) :|: 1 -A >= 0
  evalperfectreturnin(A,B,C,D) -> Com_1(evalperfectstop(NoDet0, NoDet1, NoDet2, NoDet3))
  evalperfectstart(A,B,C,D) -> Com_1(evalperfectentryin(A, B, C, D))
)
