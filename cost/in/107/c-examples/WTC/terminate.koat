(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A' B' C' NoDet1 NoDet0 NoDet2)
(RULES 
  pyRinit(A,B,C) -> Com_1(evalterminatestart(A,B,C))
  evalterminatebb1in(A,B,C) -> Com_1(evalterminatebbin(A, B, C)) :|: 100 -B >= 0 && A -C >= 0
  evalterminatebb1in(A,B,C) -> Com_1(evalterminatereturnin(A, B, C)) :|: B -101 >= 0
  evalterminatebb1in(A,B,C) -> Com_1(evalterminatereturnin(A, B, C)) :|: C -A -1 >= 0
  evalterminatebbin(A,B,C) -> Com_1(evalterminatebb1in(A -1, C, B + 1))
  evalterminateentryin(A,B,C) -> Com_1(evalterminatebb1in(B, A, C))
  evalterminatereturnin(A,B,C) -> Com_1(evalterminatestop(NoDet0, NoDet1, NoDet2))
  evalterminatestart(A,B,C) -> Com_1(evalterminateentryin(A, B, C))
)
