(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B A' B')
(RULES 
  pyRinit(A,B) -> Com_1(start(A,B))
  eval(A,B) -> Com_1(eval(A -1, B)) :|: A + B -1 >= 0 && A -B -1 >= 0
  eval(A,B) -> Com_1(eval(A -1, B)) :|: 2 * A -1 >= 0 && B -A = 0
  eval(A,B) -> Com_1(eval(A, B -1)) :|: A + B -1 >= 0 && B -A >= 0 && B -A -1 >= 0
  start(A,B) -> Com_1(eval(A, B))
)
