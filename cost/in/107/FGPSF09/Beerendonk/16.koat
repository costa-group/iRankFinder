(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A' B' C')
(RULES 
  pyRinit(A,B,C) -> Com_1(start(A,B,C))
  eval(A,B,C) -> Com_1(eval(A -1, B, C)) :|: A + B -C -1 >= 0 && C >= 0 && A -1 >= 0
  eval(A,B,C) -> Com_1(eval(A, B -1, C)) :|: A + B -C -1 >= 0 && C >= 0 && -A >= 0 && B -1 >= 0
  start(A,B,C) -> Com_1(eval(A, B, C))
)
