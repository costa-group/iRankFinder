(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A' B' C')
(RULES 
  pyRinit(A,B,C) -> Com_1(start(A,B,C))
  eval1(A,B,C) -> Com_1(eval2(A, B, C)) :|: A -B -1 >= 0
  eval2(A,B,C) -> Com_1(eval1(A -1, B, C)) :|: A -B -1 >= 0 && B -C >= 0
  eval2(A,B,C) -> Com_1(eval2(A, B, C -1)) :|: A -B -1 >= 0 && C -B -1 >= 0
  start(A,B,C) -> Com_1(eval1(A, B, C))
)
