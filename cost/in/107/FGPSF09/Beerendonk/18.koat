(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B A' B')
(RULES 
  pyRinit(A,B) -> Com_1(start(A,B))
  eval(A,B) -> Com_1(eval(A -1, B)) :|: A -1 >= 0
  eval(A,B) -> Com_1(eval(A -1, B)) :|: B -1 >= 0 && A -1 >= 0
  eval(A,B) -> Com_1(eval(A, B -1)) :|: B -1 >= 0 && -A >= 0
  start(A,B) -> Com_1(eval(A, B))
)
