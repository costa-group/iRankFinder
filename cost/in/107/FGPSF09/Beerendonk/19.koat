(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A' B' C')
(RULES 
  pyRinit(A,B,C) -> Com_1(start(A,B,C))
  eval(A,B,C) -> Com_1(eval(A -1, B, C)) :|: A -B -1 >= 0
  eval(A,B,C) -> Com_1(eval(A -1, B, C)) :|: C -B -1 >= 0 && A -B -1 >= 0
  eval(A,B,C) -> Com_1(eval(A, B, C -1)) :|: C -B -1 >= 0 && B -A >= 0
  start(A,B,C) -> Com_1(eval(A, B, C))
)
