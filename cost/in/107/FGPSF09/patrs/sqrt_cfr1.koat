(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C D A_P B_P C_P D_P NoDet1 NoDet0 NoDet2 NoDet3)
(RULES 
  pyRinit(A,B,C,D) -> Com_1(n_sqrt(A,B,C,D))
  n_f___2(A,B,C,D) -> Com_1(n_end___1(NoDet0, NoDet1, NoDet2, NoDet3)) :|: C -D -1 >= 0 && 2 * A -B + 1 = 0
  n_f___2(A,B,C,D) -> Com_1(n_f___2(A + 1, 2 * A + 3, 2 * A + C + 3, D)) :|: 2 * A + 1 >= 0 && -C + D >= 0 && 2 * A -B + 1 = 0
  n_sqrt(A,B,C,D) -> Com_1(n_f___2(0, 1, 1, D))
)
