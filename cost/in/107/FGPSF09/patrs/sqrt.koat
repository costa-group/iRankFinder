(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C D A' B' C' D' NoDet1 NoDet0 NoDet2 NoDet3)
(RULES 
  pyRinit(A,B,C,D) -> Com_1(sqrt(A,B,C,D))
  f(A,B,C,D) -> Com_1(end(NoDet0, NoDet1, NoDet2, NoDet3)) :|: C -D -1 >= 0
  f(A,B,C,D) -> Com_1(f(A + 1, B + 2, C + B + 2, D)) :|: D -C >= 0 && B >= 0
  sqrt(A,B,C,D) -> Com_1(f(0, 1, 1, D))
)
