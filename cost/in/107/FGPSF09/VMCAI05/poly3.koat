(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A' B' C')
(RULES 
  pyRinit(A,B,C) -> Com_1(start(A,B,C))
  eval(A,B,C) -> Com_1(eval(A -1, B, C -1)) :|: A >= 0 && B * B * B -C >= 0
  eval(A,B,C) -> Com_1(eval(A, B + C, C -1)) :|: A >= 0 && B * B * B -C >= 0
  start(A,B,C) -> Com_1(eval(A, B, C))
)
