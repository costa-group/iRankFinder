(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C A' B' C')
(RULES 
  pyRinit(A,B,C) -> Com_1(eval0(A,B,C))
  eval0(A,B,C) -> Com_1(eval1(A, B, C)) :|: A -1 >= 0
  eval1(A,B,C) -> Com_1(eval1(A, B + A, C)) :|: A -B -1 >= 0 && C -A -1 >= 0 && A -1 >= 0
  eval1(A,B,C) -> Com_1(eval1(A, B, B -A)) :|: A -B -1 >= 0 && C -A -1 >= 0 && A -1 >= 0
)
