(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C D A' B' C' D' NoDet2 E NoDet3 NoDet0 NoDet1)
(RULES 
  pyRinit(A,B,C,D) -> Com_1(start0(A,B,C,D))
  a(A,B,C,D) -> Com_1(b(A, B, E, D -1)) :|: A -D >= 0 && B >= 0 && C >= 0 && D -1 >= 0
  b(A,B,C,D) -> Com_1(start(A, B, C, D)) :|: C >= 0 && D >= 0 && B >= 0 && A -D -1 >= 0
  b(A,B,C,D) -> Com_1(stop3(NoDet0, NoDet1, NoDet2, NoDet3)) :|: -1 -C >= 0 && D >= 0 && B >= 0 && A -D -1 >= 0
  cont1(A,B,C,D) -> Com_1(a(A, B, C -1, D)) :|: C -1 >= 0 && D -1 >= 0 && C >= 0 && B >= 0 && A -D >= 0
  cont1(A,B,C,D) -> Com_1(stop2(NoDet0, NoDet1, NoDet2, NoDet3)) :|: D -1 >= 0 && B >= 0 && A -D >= 0 && C = 0
  start(A,B,C,D) -> Com_1(cont1(A, B, C, D)) :|: D -1 >= 0 && A >= 0 && B >= 0 && C >= 0 && D >= 0 && A -D >= 0
  start(A,B,C,D) -> Com_1(stop1(NoDet0, NoDet1, NoDet2, NoDet3)) :|: A >= 0 && B >= 0 && C >= 0 && D = 0
  start0(A,B,C,D) -> Com_1(start(A, B, B, A)) :|: A >= 0 && B >= 0
)
