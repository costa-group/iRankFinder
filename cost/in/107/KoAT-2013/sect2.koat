(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C D A' B' C' D')
(RULES 
  pyRinit(A,B,C,D) -> Com_1(l0(A,B,C,D))
  l0(A,B,C,D) -> Com_1(l1(0, B, C, D))
  l1(A,B,C,D) -> Com_1(l1(A + 1, B -1, C, D)) :|: B -1 >= 0
  l1(A,B,C,D) -> Com_1(l2(A, B, A, D)) :|: -B >= 0
  l2(A,B,C,D) -> Com_1(l3(A, B, C, C)) :|: C -1 >= 0
  l3(A,B,C,D) -> Com_1(l2(A, B, C -1, D)) :|: -D >= 0 && C -1 >= 0
  l3(A,B,C,D) -> Com_1(l3(A, B, C, D -1)) :|: D -1 >= 0 && C -1 >= 0
)
