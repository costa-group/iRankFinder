(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B C D A_P B_P C_P D_P)
(RULES 
  pyRinit(A,B,C,D) -> Com_1(n_l0(A,B,C,D))
  n_l0(A,B,C,D) -> Com_1(n_l1___4(0, B, C, D))
  n_l1___3(A,B,C,D) -> Com_1(n_l1___3(A + 1, B -1, C, D)) :|: B >= 0 && B >= 0 && B >= 0 && A >= 0 && B -1 >= 0 && B >= 0 && -1 + A >= 0
  n_l1___3(A,B,C,D) -> Com_1(n_l2___2(A, B, A, D)) :|: B >= 0 && B >= 0 && B >= 0 && A >= 0 && -B >= 0 && B >= 0 && -1 + A >= 0
  n_l1___4(A,B,C,D) -> Com_1(n_l1___3(A + 1, B -1, C, D)) :|: A >= 0 && B -1 >= 0 && A = 0
  n_l1___4(A,B,C,D) -> Com_1(n_l2___2(A, B, A, D)) :|: A >= 0 && -B >= 0 && A = 0
  n_l2___2(A,B,C,D) -> Com_1(n_l3___1(A, B, C, C)) :|: C >= 0 && 1 -B > 0 && C >= 0 && A -C >= 0 && C -1 >= 0 && -B >= 0 && -B >= 0 && C >= 0 && A -C >= 0
  n_l3___1(A,B,C,D) -> Com_1(n_l2___2(A, B, C -1, D)) :|: C >= 0 && D >= 0 && C >= 0 && D >= 0 && C -1 >= 0 && A -C >= 0 && -D >= 0 && -B >= 0 && -B >= 0 && C -D >= 0 && -1 + C >= 0 && A -C >= 0
  n_l3___1(A,B,C,D) -> Com_1(n_l3___1(A, B, C, D -1)) :|: C >= 0 && D >= 0 && C >= 0 && D >= 0 && A -C >= 0 && C -D >= 0 && D -1 >= 0 && -B >= 0 && -B >= 0 && C -D >= 0 && -1 + C >= 0 && A -C >= 0
)
