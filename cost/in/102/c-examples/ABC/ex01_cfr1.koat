(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B A_P B_P NoDet0 NoDet1)
(RULES 
  pyRinit(A,B) -> Com_1(n_evalfstart(A,B))
  n_evalfbb1in___2(A,B) -> Com_1(n_evalfbb1in___2(A, B_P)) :|: 1 + A -B >= 0 && A -B_P + 1 >= 0 && B -B_P + 1 = 0
  n_evalfbb1in___2(A,B) -> Com_1(n_evalfstop___1(NoDet0, NoDet1)) :|: 1 + A -B >= 0 && -A + B -1 >= 0
  n_evalfbb1in___3(A,B) -> Com_1(n_evalfbb1in___2(A, B_P)) :|: A -B_P + 1 >= 0 && B -B_P + 1 = 0
  n_evalfbb1in___3(A,B) -> Com_1(n_evalfstop___1(NoDet0, NoDet1)) :|: -A + B -1 >= 0
  n_evalfstart(A,B) -> Com_1(n_evalfbb1in___3(B, A))
)
