(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS pyRinit))
(VAR A B A_P B_P NoDet1 NoDet0)
(RULES 
  pyRinit(A,B) -> Com_1(n_start(A,B))
  n_start(A,B) -> Com_1(n_sumto___3(A, B))
  n_sumto___1(A,B) -> Com_1(n_end___2(NoDet0, NoDet1)) :|: 1 -A + B >= 0 && 1 -A + B >= 0 && A -B -1 >= 0 && 1 -A + B >= 0
  n_sumto___1(A,B) -> Com_1(n_sumto___1(A + 1, B)) :|: 1 -A + B >= 0 && 1 -A + B >= 0 && -A + B >= 0 && 1 -A + B >= 0
  n_sumto___3(A,B) -> Com_1(n_end___2(NoDet0, NoDet1)) :|: A -B -1 >= 0
  n_sumto___3(A,B) -> Com_1(n_sumto___1(A + 1, B)) :|: -A + B >= 0
)
